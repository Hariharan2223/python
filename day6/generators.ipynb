   "source": [
    "def arith_generators(num):\n",
    "    for i in num:\n",
    "        yield i*i\n",
    "        yield i+i\n",
    "        yield i/i\n",
    "\n",
    "a=[1,2,3,4,5]\n",
    "res=arith_generators(a)\n"
   ]
  },
